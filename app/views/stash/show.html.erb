<h1><%=@stash.name%></h1>
<div style="margin-left:20px;">
  <% Post.where(stash_id: @stash.id).each do |post|%>
  <div class="col-sm-3 post">
	  <div class="thumbnail">
	    <a href="= post.url">
			<img src="http://free.pagepeeker.com/v2/thumbs.php?size=m&url=<%=post.url%>"/>
	    </a>
	    <div class="caption">
	      <h3 class="postTitle"> <%= post.url %> </h3>
	    </div>
	  </div>
  </div>
</div>