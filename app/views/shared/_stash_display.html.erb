      <% stashes.each do |stash| %>
        <div class="col-xs-3" style="width:240px">
          <div class="thumbnail">
            <a href="/stash/<%= stash.id%>">
            <% if stash.posts.first%>
            <img src="http://api.webthumbnail.org/?width=200&format=png&url=<%=stash.posts.first.url%>"/>
            <% else %>
            <img src="https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcQSqrHNVXtTV-lls5bPViN_Y5Y6_2ugnwwJcD0Dg5sueXbLQ9ZuuA"/>
            <% end %>            </a>
            <div id="like-<%=stash.id%>" class="btn btn-default btn-lg
            like"  >
<% if current_user%>
                <span id="icon-<%=stash.id%>" class="glyphicon 
                                                     <% if
            StashLike.exists?(:stash_id=>stash.id,:user_id=>current_user.id)%>glyphicon-heart<%else%>glyphicon-heart-empty<%end%>"></span>
<% end %>
                <span id="num-likes-<%=stash.id%>"> <%=stash.score%></span>
            </div>
            <div class="caption">
              <h3 class="postTitle"> <%= stash.name %> </h3>
              <p class="postAuthor"> <%= link_to User.find(stash.user_id).name, user_path(stash.user_id)%></p>
            </div>
          </div>
        </div>
      <% end %>
