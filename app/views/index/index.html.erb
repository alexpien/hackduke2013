<br><br>
<div class="pageTitle">
	<h1>Popular Stashes</h1>
</div>

<div class="row" style="margin-left:15px;"> <!-- Your Stashes -->
  <% @stashes.each do |stash| %>
    <div class="col-sm-4 post">
      <div class="thumbnail">
        <a href="/stash/<%= stash.id%>">
			<img src="http://free.pagepeeker.com/v2/thumbs.php?size=m&url=<%=stash.posts.first.url%>"/>
        </a>
        <div class="caption">
          <h3 class="postTitle"> <%= stash.name %> </h3>
        </div>
      </div>
    </div>
  <% end %>
</div>

<div class="row" id="newsfeed"> <!-- Top rated / favorites -->
    <div class="col-sm-4 post">
    	<div class="thumbnail">
	    	<a href="http://i.imgur.com/Is3H5JH.png">
	    		<img class="postimage" src="http://i.imgur.com/Is3H5JH.png">
	    	</a>
        	<button type="button" class="btn btn-default btn-lg like" value="not liked" onClick="like(this)">
        		<span class="glyphicon glyphicon-heart-empty"></span>
        	</button>
